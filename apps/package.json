{
  "name": "@jwchat/apps",
  "version": "2.0.2",
  "private": true,
  "auther": "Bian<389701057@qq.com>",
  "description": "Vue 聊天组件",
  "keyword": "chat,Vue",
  "type": "module",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/JwChat.es.js",
      "require": "./lib/JwChat.umd.js",
      "types": "./lib/JwChat.d.ts"
    },
    "./lib/*.js": {
      "import": "./lib/JwChat.es.js",
      "require": "./lib/JwChat.umd.js",
      "types": "./lib/JwChat.d.ts"
    },
    "./*": "./*"
  },
  "main": "./lib/JwChat.umd.js",
  "module": "./lib/JwChat.es.js",
  "types": "./lib/JwChat.d.ts",
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "dev": "vite",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite build --mode build && node script/copyDir.js",
    "lib": "vue-tsc --noEmit && vite build --mode lib && rm -rf ./lib/image",
    "gen": "node ./script/genNewComp/index.js",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "install:all": "pnpm install",
    "build:all": "pnpm -r build"
  },
  "dependencies": {
    "@better-scroll/core": "^2.4.2",
    "@better-scroll/mouse-wheel": "^2.4.2",
    "@better-scroll/observe-dom": "^2.4.2",
    "@better-scroll/observe-image": "^2.4.2",
    "@better-scroll/pull-down": "^2.4.2",
    "@better-scroll/scroll-bar": "^2.4.2",
    "@better-scroll/shared-utils": "^2.5.1",
    "@element-plus/icons-vue": "^2.0.4",
    "@vueuse/core": "^10.11.0",
    "element-plus": "^2.1.9",
    "jwchat": "workspace:*",
    "prismjs": "^1.25.0",
    "push_npm": "workspace:*",
    "vue": "^3.2.37",
    "wechat-emoji-parser": "^2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^16.11.12",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "conventional-changelog": "^3.1.25",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.9.1",
    "fs-extra": "^10.0.0",
    "git-cz": "4.8.0",
    "handlebars": "^4.7.7",
    "husky": "9.0.11",
    "inquirer": "^8.2.0",
    "sass": "^1.50.0",
    "typescript": "^4.4.4",
    "unplugin-element-plus": "^0.8.0",
    "vite": "^5.4.19",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-dts": "^0.9.6",
    "vite-plugin-md": "^0.11.4",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.28.10"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}