{
  "name": "JChat",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "element-plus": "^2.10.2",
    "typescript": "^4.9.5",
    "vue": "^3.5.17"
  },
  "scripts": {
    "bootstrap": "pnpm -F jwchat i && pnpm -F jwchatdoc i",
    "stub": "pnpm run -r --parallel stub",
    "postinstall": "pnpm stub",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "vue-demi"
    ]
  },
  "devDependencies": {
    "rimraf": "^6.0.1"
  }
}